{
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ],
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": true,
        "regenerator": true
      }
    ],
    ["transform-decorators-legacy"]
  ],
  "presets": ["react", ["es2015", { "modules": false }], "stage-0"],
  "ignore": ["node_modules/**"]
}
